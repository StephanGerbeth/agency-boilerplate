---
touch-indicator: false
id: #string
class: #string
url: "http://www."
title: "Link"
target: "_self"
rel: "next"
---
<a href="{{{url}}}"
    {{#if id}}
        id="{{{id}}}"
    {{/if}}
    class="partial{{#if class}} {{class}}{{/if}}"
    title="{{{title}}}"
    rel="{{{rel}}}"
    {{#block "attributes"}}
        target="{{{target}}}"
        data-partial="elements/link"
    {{/block}}
>{{#if touch-indicator}}{{{mixin "services/touch-indicator" touch-indicator}}}{{/if}}{{#block "status"}}{{{status}}}{{/block}}{{#block "title"}}{{{title}}}{{/block}}</a>
